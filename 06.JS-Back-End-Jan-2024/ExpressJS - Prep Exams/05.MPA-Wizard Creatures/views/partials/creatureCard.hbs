<div class="flip flip-vertical">
  <div class="front">
    <img src="{{imageURL}}" alt="{{name}}">

  </div>
  <div class="back">
    <h1>{{name}}</h1>
    <h2>Species: {{species}}</h2>
    <p>Description: {{description}}</p>
    <a href="/catalog/details/{{_id}}" class="details">Details</a>
  </div>
</div>  