<section id="create-container">
  <div class="create-container-info">
    <h1>Create Volcano Wiki</h1>
    <form action="/catalog/create" method="post">
      <label>Name:</label>
      <input type="text" id="name" name="name" placeholder="..." value="{{body.name}}">
      <label>Location:</label>
      <input type="text" id="location" name="location" placeholder="..." value="{{body.location}}">
      <label>Elevation:</label>
      <input type="number" id="elevations" name="elevation" placeholder="..." value="{{body.elevation}}">
      <label>Year of Last Eruption:</label>
      <input type="number" id="last-year-eruption" name="lastEruption" placeholder="..." value="{{body.lastEruption}}">
      <label>Image:</label>
      <input type="text" id="image" name="imageURL" placeholder="http://..." value="{{body.imageURL}}">
      <label>Volcano Type:</label>
      <select id="type" name="typeVolcano">
        <option value="Supervolcanoes">Supervolcanoes</option>
        <option value="Submarine">Submarine</option>
        <option value="Subglacial">Subglacial</option>
        <option value="Mud">Mud</option>
        <option value="Stratovolcanoes">Stratovolcanoes</option>
        <option value="Shield">Shield</option>
      </select>
      <label>Description:</label>
      <textarea id="description" name="description" placeholder="...">{{body.description}}</textarea>
      <input type="submit" id="btn" value="Submit"></input>
    </form>
  </div>
</section>